{
  "name": "open-payments-php/snippets",
  "description": "A simple PHP Open Payments console application",
  "type": "project",
  "require": {
      "php": "^8.3",
      "symfony/console": "^6.3",
    "vlucas/phpdotenv": "^5.6"
  },
  "autoload": {
      "psr-4": {
          "App\\": "src/",
          "OpenPayments\\": "vendor/open-payments-php/lib"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "App\\Tests\\": "tests/"
      }
  },
  "scripts": {
      "test": "phpunit"
  }
}
